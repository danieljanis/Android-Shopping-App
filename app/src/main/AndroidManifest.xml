<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="edu.danieljanis.shoppingmvvm">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name=".ListApplication"
        tools:ignore="AllowBackup">
        <!--suppress AndroidUnknownAttribute -->
        <activity android:name=".lists.ItemsActivity" android:configChanges="orientation|screenSize|keyboardHidden"/>
        <activity android:name=".titles.TitlesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>